<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--
            The CKSetup is done on the DBWithPasswordAndGoogle bin folder with CK.DB.User.UserPassword
            and CK.DB.User.UserGoogle in it.
            With NetCore 2.1, DBWithPasswordAndGoogle can simply target "netstandard2.0" (with
            <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>). Before (NetCore 2.0), it has to
            target netcoreapp2.0.
        -->
    <add key="CKSetup/DefaultBinPaths" value="../DBWithPasswordAndGoogle/bin/{BuildConfiguration}/netstandard2.0" />
    <add key="Monitor/LogToTextFile" value="true" />
    <add key="Monitor/LogLevel" value="Debug" />
    <add key="CKSetup/WorkingDirectory" value="CKSetupWorkingDir" />
  </appSettings>
</configuration>
